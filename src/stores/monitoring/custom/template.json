{
  "Elasticsearch Overview": {
    "settings": {
      "datasource": "prometheus",
      "panels": [
        {
          "bars": false,
          "colors": [
            "#60acfc",
            "#23c2db",
            "#64d5b2",
            "#d5ec5a",
            "#ffb64e",
            "#fb816d",
            "#d15c7f"
          ],
          "description": "",
          "id": 9,
          "legend": {
            "avg": true,
            "current": false,
            "max": true,
            "min": true,
            "show": true,
            "total": false,
            "values": true
          },
          "lines": true,
          "stack": false,
          "targets": [
            {
              "expr": "irate(elasticsearch_jvm_gc_collection_seconds_count[2m])",
              "legendFormat": "{name} - {gc}",
              "refId": 1,
              "step": "20s"
            }
          ],
          "title": "GC count",
          "type": "graph",
          "xaxis": {
            "show": true
          },
          "yaxes": [
            {
              "decimals": 3,
              "format": "none",
              "show": true
            },
            {
              "show": false
            }
          ]
        },
        {
          "bars": false,
          "colors": [
            "#60acfc",
            "#23c2db",
            "#64d5b2",
            "#d5ec5a",
            "#ffb64e",
            "#fb816d",
            "#d15c7f"
          ],
          "description": "",
          "id": 10,
          "legend": {
            "avg": true,
            "current": false,
            "max": true,
            "min": true,
            "show": true,
            "total": false,
            "values": true
          },
          "lines": true,
          "stack": false,
          "targets": [
            {
              "expr": "elasticsearch_os_load1",
              "legendFormat": "load1: {name}",
              "refId": 1,
              "step": "20s"
            },
            {
              "expr": "elasticsearch_os_load5",
              "legendFormat": "load5: {name}",
              "refId": 2,
              "step": "20s"
            },
            {
              "expr": "elasticsearch_os_load15",
              "legendFormat": "load15: {name}",
              "refId": 3,
              "step": "20s"
            }
          ],
          "title": "Load average",
          "type": "graph",
          "xaxis": {
            "show": true
          },
          "yaxes": [
            {
              "decimals": 2,
              "format": "none",
              "show": true
            },
            {
              "show": false
            }
          ]
        },
        {
          "decimals": 2,
          "format": "percent",
          "id": 2,
          "targets": [
            {
              "expr": "sum (elasticsearch_process_cpu_percent) / count (elasticsearch_process_cpu_percent)"
            }
          ],
          "title": "CPU usage Avg",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 2,
          "format": "percent",
          "id": 1,
          "targets": [
            {
              "expr": "sum (elasticsearch_jvm_memory_used_bytes) / sum (elasticsearch_jvm_memory_max_bytes) "
            }
          ],
          "title": "JVM memory used Avg",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 0,
          "format": "none",
          "id": 3,
          "targets": [
            {
              "expr": "elasticsearch_cluster_health_number_of_nodes"
            }
          ],
          "title": "Healthy Nodes",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 0,
          "format": "none",
          "id": 4,
          "targets": [
            {
              "expr": "elasticsearch_cluster_health_number_of_pending_tasks"
            }
          ],
          "title": "Pending tasks",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 0,
          "format": "none",
          "id": 5,
          "targets": [
            {
              "expr": "sum (elasticsearch_process_open_files_count)"
            }
          ],
          "title": "Open file descriptors per cluster",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 0,
          "format": "none",
          "id": 6,
          "targets": [
            {
              "expr": "elasticsearch_cluster_health_active_primary_shards"
            }
          ],
          "title": "Active primary shards",
          "type": "singlestat",
          "valueName": "avg"
        },
        {
          "decimals": 0,
          "format": "none",
          "id": 7,
          "targets": [
            {
              "expr": "elasticsearch_cluster_health_active_shards"
            }
          ],
          "title": "Active shards",
          "type": "singlestat",
          "valueName": "avg"
        }
      ],
      "refresh": "",
      "templatings": [],
      "time": {
        "from": "now-30m",
        "to": "now"
      },
      "title": "Elasticsearch Overview"
    },
    "description": "elasticsearch-exporter",
    "name": "Elasticsearch",
    "logo": "/assets/elasticsearch.svg"
  },
  "MySQL Overview": {
