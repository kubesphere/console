  "scripts": {
    "start": "concurrently -k --raw 'yarn dev:client' 'yarn dev:server'",
    "build": "yarn build:client && yarn build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.dev.js -w",
    "dev:server": "cross-env NODE_ENV=development nodemon -w ./common -w ./server -e js,yaml server/server.js",
    "build:client": "yarn lego && webpack -p --config scripts/webpack.prod.js --display errors-only",
    "build:server": "webpack -p --config scripts/webpack.server.js --display errors-only",
    "build:e2e": "yarn lego && webpack -p --config scripts/webpack.e2e.js --display errors-only",
    "test": "jest",
    "test:e2e": "cypress run",
    "serve": "NODE_ENV=production node server/server.js",
    "lego": "webpack -p --config scripts/webpack.lego.js",
    "lint": "eslint ./src",
    "postinstall": "rimraf .cache-loader",
    "prebuild": "rimraf dist",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json, css, scss}": [
      "prettier --write",
      "git add"
    ]
  },
